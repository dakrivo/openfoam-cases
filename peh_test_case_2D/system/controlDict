FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     simpleFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         15000;

deltaT          1;

writeControl    timeStep;

writeInterval   1000;

purgeWrite      3;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

functions
{
    solverInfo
    {
        type            solverInfo;
        libs ("libutilityFunctionObjects.so");
        enabled         true;
        writeControl    timeStep;
		writeInterval   1;

        fields
        (
            p
            U
            k
			omega
        );
		writeResidualFields no;
    }
	

}

libs
(
    "pressureGradientExplicitSource.so"
);

// ************************************************************************* //
